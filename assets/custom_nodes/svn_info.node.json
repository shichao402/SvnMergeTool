{
  "typeId": "svn_info",
  "name": "SVN 信息",
  "description": "获取 SVN 工作副本的详细信息，包括版本号、URL、最后修改者等",
  "icon": "info",
  "color": "blue",
  "category": "SVN 操作",
  "inputs": [
    {
      "id": "in",
      "name": "输入",
      "description": "接收上游数据"
    }
  ],
  "outputs": [
    {
      "id": "success",
      "name": "成功",
      "description": "成功获取 SVN 信息"
    },
    {
      "id": "failure",
      "name": "失败",
      "description": "获取信息失败"
    }
  ],
  "params": [
    {
      "key": "wcPath",
      "name": "工作副本路径",
      "type": "string",
      "defaultValue": "",
      "description": "SVN 工作副本路径，留空则使用任务的目标工作副本"
    }
  ],
  "executor": {
    "type": "shell",
    "command": "svn info ${config.wcPath:-${job.targetWc}}",
    "timeout": 60,
    "outputParser": {
      "revision": "regex:Revision: (\\d+)",
      "url": "regex:URL: (.+)",
      "lastAuthor": "regex:Last Changed Author: (.+)",
      "lastDate": "regex:Last Changed Date: (.+)"
    },
    "portMapping": {
      "exitCode": {
        "0": "success",
        "*": "failure"
      }
    }
  }
}
