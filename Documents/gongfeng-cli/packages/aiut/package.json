{
  "name": "@tencent/gongfeng-cli-plugin-aiut",
  "version": "0.25.3-beta.0",
  "description": "Core aiut plugin for GongFeng CLI",
  "author": "ajunhuang",
  "bin": {
    "gf": "./bin/run"
  },
  "homepage": "https://git.woa.com/code/code-frontend/gongfeng-cli/tree/master/packages/aiut",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://git.woa.com/code/code-frontend/gongfeng-cli/tree/master/packages/aiut",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@sentry/node": "^7.1.1",
    "@tencent/gongfeng-cli-base": "^0.21.0-beta.0",
    "@types/i18n": "^0.13.2",
    "@types/marked": "^4.0.8",
    "@types/marked-terminal": "^3.1.3",
    "axios": "^0.27.2",
    "debug": "^4.3.4",
    "fs-extra": "^10.1.0",
    "git-url-parse": "^13.1.0",
    "i18n": "^0.15.0",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.21",
    "marked": "^4.2.12",
    "marked-terminal": "^5.1.1",
    "oclif": "^3",
    "ora": "5.0.0",
    "qs": "^6.11.1",
    "split2": "^4.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.20",
    "@types/chai": "^4",
    "@types/git-url-parse": "^9.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.31",
    "@types/split2": "^3.2.1",
    "chai": "^4",
    "mocha": "^9",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "gf",
    "dirname": "gf",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "copy": "copyfiles locales/*.json dist",
    "build": "shx rm -rf dist && tsc -b && copyfiles locales/*.json dist",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "types": "dist/index.d.ts"
}
