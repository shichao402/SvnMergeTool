{
  "name": "@tencent/gongfeng-cli-plugin-auth",
  "version": "0.21.0-beta.0",
  "description": "core auth plugin for GongFeng CLI",
  "author": "GongFeng developers",
  "homepage": "https://git.woa.com/code/code-frontend/gongfeng-cli/tree/master/packages/auth",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "packages/auth",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@sentry/node": "^7.1.1",
    "@tencent/gongfeng-cli-base": "^0.21.0-beta.0",
    "@tencent/gongfeng-ioa-login": "^1.0.13",
    "@types/i18n": "^0.13.2",
    "axios": "^0.27.2",
    "copyfiles": "^2.4.1",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "i18n": "^0.15.0",
    "inquirer": "^7.3.3",
    "netrc-parser": "^3.1.6",
    "ora": "5.0.0",
    "qrcode": "^1.5.1",
    "qs": "^6.10.3",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.20",
    "@tencent/tg-lint": "^0.6.0",
    "@types/chai": "^4",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.31",
    "@types/qrcode": "^1.5.0",
    "@types/qs": "^6.9.7",
    "chai": "^4",
    "globby": "^11",
    "mocha": "^9",
    "nyc": "^15.1.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "gf",
    "dirname": "gf",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "copy": "copyfiles locales/*.json dist",
    "build": "shx rm -rf dist && tsc -b && copyfiles locales/*.json dist",
    "lint": "eslint . --ext .ts --config .eslintrc.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "bugs": "https://github.com/packages/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
